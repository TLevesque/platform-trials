{
  "name": "custom-server-koa",
  "version": "1.0.0",
  "scripts": {
    "dev": "node --experimental-modules server.mjs",
    "build": "next build",
    "start": "NODE_ENV=production node --experimental-modules server.mjs",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "analyze:size": "cross-env ANALYZE=1 next build && cat .next/stats.txt"
  },
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "dotenv-webpack": "^1.5.7",
    "koa": "^2.6.2",
    "koa-router": "^7.4.0",
    "lodash-es": "^4.17.11",
    "next": "^7.0.2-canary.34",
    "next-server": "^7.0.2-canary.34",
    "next-compose-plugins": "^2.1.1",
    "next-images": "^1.0.1",
    "next-plugin-transpile-modules": "^0.1.3",
    "node-sass": "^4.10.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "webpack-bundle-size-analyzer": "^3.0.0"
  },
  "devDependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "cross-env": "^5.2.0"
  }
}

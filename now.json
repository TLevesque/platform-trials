{
  "version": 2,
  "name": "platform-trials",
  "regions": ["bru"],
  "public": false,
  "builds": [
    {
      "src": "server.mjs",
      "use": "@now/node",
      "config": {
        "maxLambdaSize": "10mb"
      }
    },
    {
      "src": "next.config.js",
      "use": "@now/next@canary"
    },
    { "src": "static/**/*", "use": "@now/static" }
  ]
}

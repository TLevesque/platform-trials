{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "static/*",
      "use": "@now/static"
    }
  ],
  "off-builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "off-routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
